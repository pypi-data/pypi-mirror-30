{
  "applications": ["guillotina_hive"],
  "databases": [{
    "db": {
      "storage": "postgresql",
      "transaction_strategy": "resolve",
      "dsn": {
        "scheme": "postgres",
        "dbname": "guillotina",
        "user": "postgres",
        "host": "localhost",
        "password": "",
        "port": 32768
      },
      "read_only": false
    }
  }],
  "hive": {
    "cluster_config": {
      "user": "",
      "credentials": "",
      "ca": "",
      "endpoint": "localhost:6443",
      "skip_ssl": true,
      "certificate": "",
      "key": ""
    }
  },
  "host": "127.0.0.1",
  "port": 8080,
  "root_user": {
    "password": "root"
  },
  "cors": {
    "allow_origin": ["*"],
    "allow_methods": ["GET", "POST", "DELETE", "HEAD", "PATCH"],
    "allow_headers": ["*"],
    "expose_headers": ["*"],
    "allow_credentials": true,
    "max_age": 3660
  },
  "utilities": [],
  "elasticsearch": {
    "bulk_size": 50,
    "index_name_prefix": "guillotina-",
    "connection_settings": {
      "endpoints": ["localhost:9200"],
      "sniffer_timeout": 0.5
    },
    "index": {},
    "mapping_overrides": {
      "*": {
      }
    }
  }
}
