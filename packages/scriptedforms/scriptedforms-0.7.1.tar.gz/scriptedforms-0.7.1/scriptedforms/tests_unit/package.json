    "css-loader": "~0.28.7",
    "es6-promise": "~4.1.1",
    "file-loader": "~0.10.1",
    "fs-extra": "~4.0.2",
    "istanbul-instrumenter-loader": "~3.0.0",
    "json-loader": "~0.5.7",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~0.2.3",
    "karma-coverage": "~0.5.5",
