{% extends "base.pdf" %}
{% comment %}
This Source Code Form is subject to the terms of the Mozilla Public
License, v. 2.0. If a copy of the MPL was not distributed with this
file, You can obtain one at http://mozilla.org/MPL/2.0/.
{% endcomment %}
<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
{% load static %}
{% load proto_tags %}
{% load l10n %}
{% load i18n proto_tags admin_static %}
{% get_static_prefix as STATIC_PREFIX %

{% block size %} a3 landscape {% endblock %}
{% block background %}  {% endblock %}
{% block top %} 1.5{% endblock %}
{% block bottom %} 0.5{% endblock %}
{% block extrahead %}
    <title>Bayfaces</title>
{% endblock %}
{% block extrastyle %}
    h1 {
        font: 16px Arial, Verdana, Helvetica, sans-serif;
        font-weight: bold;
        line-height: 1;
        color:#134089;
        margin:0;
        display:block;
    }
    h2 {
        font: 15px Arial, Verdana, Helvetica, sans-serif;
        font-weight: bold;
        line-height: 1;
        color:#333;
    }
    
    .rack h2 {
        margin-left:42px;
    } 
    
    .tabela tr { height:5px; }
    
    .u {
        background:white repeat-y;
        display: table-cell;
        width:7px;
        vertical-align:top;
        background-image: url('static:1u.png');
    }
    .bloco-n {
        display: table-cell;
        width:20px;
        background-color:white;
        margin-right:7px;
        vertical-align:top;
        border-bottom:1px solid;
        border-color: #999a9a;
        
    }
    .bloco-n>div {
        position:relative;
    }
    
    .bloco-n .u-n {
        background:#d8d8d8;
        color:#636363;
        font:8px Arial, Verdana, Helvetica, sans-serif;
        border-top:0;
        border-right:1px;
        border-bottom:0;
        border-left:1px; 
        border-style:solid; 
        height:19px;
    }
    .bloco-n .u-n div {
        border-top: 1px solid;
        vertical-align: middle;
        text-align: center;
        line-height: 18px;
    }
    /**
    Coluna de furos do rack
    */
    .u-n-left {
        width:7px; 
        display:block; 
        background-color:#eee; 
        position:absolute; 
        left:24px; 
        height: 100%; 
        top:0px; 
        border-left:#aaa 2px solid; 
        border-right: #888 2px solid;
        border-top:0;
        border-bottom:0;
    }
    .u-n-right {
        width:7px; 
        display:block; 
        background-color:#eee; 
        position:absolute; 
        left:239px; 
        height: 100%; 
        top:0; 
        border-left:#aaa 2px solid; 
        border-right: #888 2px solid;
    }
    .u-n-f {
        background:black; 
        position:absolute; 
        width:1px; 
        height:1px;
        padding-top:1px;
        padding-left:1px;
        left:1px;
        border:0; 
    }
    .f1 {
        top:3px;
    }
    .f2 {
        top:9px;
    }
    .f3 {
        top:15px;
    }
    
    .rack {
        display:inline-block;
        margin-right:25px;
        font-size:0;
        position:relative;
    }
    .outros,
    .conflitos {
        display:inline-block;
        width:255px;
    }
    .equip.profundidade_curta {
        background-color:#ccc;
    }
    img.profundidade_curta {
        opacity:0.3;
    }
    .interno {
        position:relative;
        display: table-cell;
        width:199px;
        padding-left:17px;
        font-family: Arial, Verdana, Helvetica, sans-serif; 
        font-size:10px;
        top:-3px;
    }
    .interno img {
        position:absolute;
        z-index: 5;
        width:100%;
        height:100%;
    }
    .interno div {
        position:absolute;
        z-index: 1;
    }
    .interno div div div:nth-child(1) {
        position:relative;
        display: table-cell;
        vertical-align:middle;
        text-align: center;
        color:#FFF;
    }
    
    .interno div div div:nth-child(2) {
        position: absolute;
        top: 0;
        left: 0;
        vertical-align:middle;
        line-height: 1.5em;
        text-align: center;
        color:#FFF;
        display:none;
    }
    
    .interno div div div {
        width:199px;
    }
    
    .equip {
        display:inline-block;
        background:#81878b;
        border-bottom:1px solid #fff;
        border-top:1px solid #fff;
        padding-right:1px; 
        padding-left:1px;
        vertical-align:middle; 
        line-height:1.5em;
        width: 199px;
        z-index:1;
        top: 5px; /* OBS: posição na tela e no PDF são diferentes. 5px para PDF e 7px para tela */
        
    }
    .equip.pos_T01,
    .equip.pos_T01 div,
    .equip.pos_01,
    .equip.pos_01 div {
        width:97px;
        font-size: 9px;
    }
    .equip.pos_T02,
    .equip.pos_T02 div div,
    .equip.pos_02,
    .equip.pos_02 div div {
        /*left:16.3em;*/
        right:43px;
        width:97px;
        padding-right: 0;
        margin-right: 0;
        font-size: 9px;
    }
    .equip.pos_T03,
    .equip.pos_T03 div,
    .equip.pos_03,
    .equip.pos_03 div {
    }
    .dc {
        margin-top:5px;
    }
    .outros caption {
        display:block;
        text-align: left;
        font-size: 12px;
        font-weight: bold;
    }
    .outros table {
        margin-left:0;
        margin-right:0;
        margin-bottom:10px;
        margin-top:10px;
        width:100%;
        border-spacing:0;
        border-collapse: collapse;
    }
    .outros th,
    .outros td {
        border: 1px solid;
        font-family: Arial, Verdana, Helvetica, sans-serif;
        font-size:11px;
    }
     .div_pdu {
        display: table-cell;
        width: 30px;
        color: black;
        font-size: 5px;
        padding-left:15px;
    }
     .div_pdu a .equip {
        width: 18px;
        margin-left: 4px;
        margin-right: 5px;
        position:absolute;
     }
     .div_pdu a .equip img {
        width: 100%;
        height: 100%;
     }
    
    
    
    .page-break {
        page-break-before:always;
        display: block;
        clear: both;
    }
     
{% endblock %}
{% block content %}
    <h1>Diagrama de Racks Frontal</h1>
    {% for dc in dcs %}
    
    {% if not forloop.first %}
        <div class="page-break"></div>
    {% endif %}
    
    
    <h1>{{ dc.entidade }} - {{ dc.nome|default:'' }}</h1>
    {% for fileira in dc.fileiras %}
        {% if not forloop.first %}
            <p class="page-break"></p>
        {% endif %}
        {% for rack in fileira.racks %}
            {% if not forloop.first %}
                {% if chk_avisos == '1' %}
                    <div class="page-break"></div>
                {% elif chk_outros == '1' and forloop.counter0|divisibleby:"2" %}
                       <div class="page-break"></div>
                {% elif forloop.counter0|divisibleby:"5" %}
                       <div class="page-break"></div>
                {% endif %}
            {% endif %}
            <div class="rack">
                <h2>
                  {{ rack.nome|default:'' }} ({{ rack.vazio }} ocupado)
                </h2>
                <div class="rack_frontal" style="display:table-row">
                    {% comment %}
                    <div class="div_pdu" style="height: {{ rack.altura_pxs|unlocalize }}px;">
                        {% for eq in rack.equipamentos_pdu %}
                            {% if eq.pos_col == 'LE' %}
                               <div class="equip " rel="tooltip" 
                                    style="margin-top:{{ eq.eixoY|unlocalize }}px; height:{{ eq.altura|unlocalize }}px;">
                                    {% if eq.imagem %}<img src="media:{{ eq.imagem }}" />{% endif %}
                               </div>
                            {% endif %}
                        {% endfor %}
                    </div>
                    {% endcomment %}
                    <div id="rack_front_{{ rack.id }}_{{forloop.counter}}" class="bloco-n" style="position:relative; height: {{ rack.altura_pxs|unlocalize }}px;">
                        {% for i in rack.altura_count reversed %}
                            <div>
                                <div class="u-n">
                                    <div>{{ i }}</div>
                                </div>
                                <div class="u-n-left">
                                    <div class="u-n-f  f1"></div>
                                    <div class="u-n-f  f2"></div>
                                    <div class="u-n-f  f3"></div>
                                </div>
                                <div class="u-n-right" >
                                    <div class="u-n-f  f1"></div>
                                    <div class="u-n-f  f2"></div>
                                    <div class="u-n-f  f3"></div>
                                </div>
                            </div>
                        {% endfor %}
                    </div>
                    
                    <div class="interno">
                        {% for eq in rack.equipamentos %}
                            <div class="{% if eq.pos_col %} pos_{{eq.pos_col}} {% endif %}  equip {% if eq.flag_traseiro and eq.profundidade < 1.0 %}profundidade_curta{% endif %}" 
                               style="margin-top:{{ eq.eixoY|unlocalize }}px; height:{{ eq.altura|unlocalize }}px; {% if not eq.flag_traseiro %} z-index:10;{% endif %}">
                                <div>
                                    {% if chk_legenda != "0" %}
                                        <div style="height:{{ eq.altura|unlocalize }}px;">
                                            {% if eq.pos_col %}
                                                {% if eq.tam >= 15 %}
                                                    {{ eq.nome|default:""|truncatechars:80 }}
                                                {% elif eq.tam >= 12 %}
                                                    {{ eq.nome|default:""|truncatechars:65 }}
                                                {% elif eq.tam >= 9 %}
                                                    {{ eq.nome|default:""|truncatechars:50 }}            
                                                {% elif eq.tam >= 6 %}
                                                    {{ eq.nome|default:""|truncatechars:35 }}
                                                {% else %}
                                                    {{ eq.nome|default:""|truncatechars:20 }}
                                                {% endif %}
                                            {% else %}
                                                {% if eq.tam >= 15 %}
                                                    {{ eq.nome|default:""|truncatechars:150 }}
                                                {% elif eq.tam >= 12 %}
                                                    {{ eq.nome|default:""|truncatechars:120 }}
                                                {% elif eq.tam >= 9 %}
                                                    {{ eq.nome|default:""|truncatechars:90 }}            
                                                {% elif eq.tam >= 6 %}
                                                    {{ eq.nome|default:""|truncatechars:60 }}
                                                {% else %}
                                                    {{ eq.nome|default:""|truncatechars:30 }}
                                                {% endif %}
                                            {% endif %}
                                        </div>
                                    {% endif %}
                                    {% if chk_legenda_desc == "1" %}
                                        <div style="height:{{ eq.altura|unlocalize }}px;" style="position:absolute; display:block;">
                                        {% if eq.pos_col %}
                                            {% if eq.tam >= 15 %}
                                                {{ eq.descricao|truncatechars:80 }}
                                            {% elif eq.tam >= 12 %}
                                                {{ eq.descricao|truncatechars:65 }}
                                            {% elif eq.tam >= 9 %}
                                                {{ eq.descricao|truncatechars:50 }}            
                                            {% elif eq.tam >= 6 %}
                                                {{ eq.descricao|truncatechars:35 }}
                                            {% else %}
                                                {{ eq.descricao|truncatechars:20 }}
                                            {% endif %}
                                        {% else %}
                                            {% if eq.tam >= 15 %}
                                                {{ eq.descricao|truncatechars:150 }}
                                            {% elif eq.tam >= 12 %}
                                                {{ eq.descricao|truncatechars:120 }}
                                            {% elif eq.tam >= 9 %}
                                                {{ eq.descricao|truncatechars:90 }}            
                                            {% elif eq.tam >= 6 %}
                                                {{ eq.descricao|truncatechars:60 }}
                                            {% else %}
                                                {{ eq.descricao|truncatechars:30 }}
                                            {% endif %}
                                        {% endif %}
                                        </div>
                                    {% endif %}
                                </div>
                                {% if chk_stencil != "0" %}
                                    {% if eq.flag_traseiro %}
                                        {% if eq.imagem_traseira %}<img src="media:{{ eq.imagem_traseira }}" {% if eq.profundidade < 1.0 %}class="profundidade_curta"{% endif %} />{% endif %}
                                    {% else %}
                                        {% if eq.imagem %}<img src="media:{{ eq.imagem }}" />{% endif %}
                                    {% endif %}
                                {% endif %}
                            </div>
                        {% endfor %}
                    </div>{# div .interno #}
                    {% comment %}
                        <div class="div_pdu" style="height: {{ rack.altura_pxs|unlocalize }}px;">
                            {% for eq in rack.equipamentos_pdu %}
                                {% if eq.pos_col == 'LD' %}
                                    <div class="equip " rel="tooltip" 
                                        style="margin-top:{{ eq.eixoY|unlocalize }}px; height:{{ eq.altura|unlocalize }}px;">
                                        {% if eq.imagem %}<img src="media:{{ eq.imagem }}" />{% endif %}
                                    </div>
                                {% endif %}
                            {% endfor %}
                        </div>
                    {% endcomment %}
                </div>{# div .rack_frontal #} 
            </div>{# div .rack #}
        {% endfor %}{# end rack #}
    {% endfor %}{# end fileira #}
{% endfor %}{# end dc #}
{% if chk_traseira %}
<div class="page-break"></div>
<h1>Diagrama de Racks Traseiro</h1>
    {% for dc in dcs %}
    {% if not forloop.first %}
        <div class="page-break"></div>
    {% endif %}
    <div class="dc">
    <h1>{{ dc.entidade }} - {{ dc.nome|default:'' }}</h1>
    {% for fileira in dc.fileiras %}
        {% if not forloop.first %}
            <p class="page-break"></p>
        {% endif %}
        {% for rack in fileira.racks %}
            {% if not forloop.first %}
                {% if chk_avisos == '1' %}
                    <div class="page-break"></div>
                {% elif chk_outros == '1' and forloop.counter0|divisibleby:"2" %}
                       <div class="page-break"></div>
                {% elif forloop.counter0|divisibleby:"5" %}
                       <div class="page-break"></div>
                {% endif %}
            {% endif %}
            <div class="rack">
              <h2>
                {{ rack.nome|default:'' }} ({{ rack.vazio }} ocupado)
              </h2>
              <div class="rack_traseiro" style="display:table-row">
                {% comment %}
                <div class="div_pdu" style="height: {{ rack.altura_pxs|unlocalize }}px;">
                    {% for eq in rack.equipamentos_pdu %}
                        {% if eq.pos_col == 'TE' %}
                           {% if eq.id %}
                               <a href="{% url "admin:patrimonio_patrimonio_change" eq.id %}" onclick="return showAddAnotherPopup(this);">
                           {% endif %}
                           <div class="equip " rel="tooltip" 
                                style="margin-top:{{ eq.eixoY|unlocalize }}px; height:{{ eq.altura|unlocalize }}px;">
                                {% if eq.imagem %}<img src="media:{{ eq.imagem }}" />{% endif %}
                           </div>
                           {% if eq.id %}
                               </a>
                           {% endif %}
                        {% endif %}
                    {% endfor %}
                </div>
                {% endcomment %}
                <div id="rack_rear_{{ rack.id }}_{{forloop.counter}}" class="bloco-n" style="position:relative; height: {{ rack.altura_pxs|unlocalize }}px;">
                    {% comment %}
                    ssss
                    {% endcomment %}
                </div>
                
                <div class="interno">
                {% for eq in rack.equipamentos %}
                   <div class="{% if eq.pos_col %} pos_{{eq.pos_col}} {% endif %}  equip  {% if not eq.flag_traseiro and eq.profundidade < 1.0 %}profundidade_curta{% endif %} " 
                       style="margin-top:{{ eq.eixoY|unlocalize }}px; height:{{ eq.altura|unlocalize }}px; {% if eq.flag_traseiro %} z-index:10;{% endif %}">
                       <div>
                           <div style="height:{{ eq.altura|unlocalize }}px;">
                            {% if eq.pos_col %}
                                {% if eq.tam >= 15 %}
                                    {{ eq.nome|default:""|truncatechars:80 }}
                                {% elif eq.tam >= 12 %}
                                    {{ eq.nome|default:""|truncatechars:65 }}
                                {% elif eq.tam >= 9 %}
                                    {{ eq.nome|default:""|truncatechars:50 }}            
                                {% elif eq.tam >= 6 %}
                                    {{ eq.nome|default:""|truncatechars:35 }}
                                {% else %}
                                    {{ eq.nome|default:""|truncatechars:20 }}
                                {% endif %}
                            {% else %}
                                {% if eq.tam >= 15 %}
                                    {{ eq.nome|default:""|truncatechars:150 }}
                                {% elif eq.tam >= 12 %}
                                    {{ eq.nome|default:""|truncatechars:120 }}
                                {% elif eq.tam >= 9 %}
                                    {{ eq.nome|default:""|truncatechars:90 }}            
                                {% elif eq.tam >= 6 %}
                                    {{ eq.nome|default:""|truncatechars:60 }}
                                {% else %}
                                    {{ eq.nome|default:""|truncatechars:30 }}
                                {% endif %}
                            {% endif %}
                            </div>
                            <div style="height:{{ eq.altura|unlocalize }}px;" style="position:absolute">
                            {% if eq.pos_col %}
                                {% if eq.tam >= 15 %}
                                    {{ eq.descricao|truncatechars:80 }}
                                {% elif eq.tam >= 12 %}
                                    {{ eq.descricao|truncatechars:65 }}
                                {% elif eq.tam >= 9 %}
                                    {{ eq.descricao|truncatechars:50 }}            
                                {% elif eq.tam >= 6 %}
                                    {{ eq.descricao|truncatechars:35 }}
                                {% else %}
                                    {{ eq.descricao|truncatechars:20 }}
                                {% endif %}
                            {% else %}
                                {% if eq.tam >= 15 %}
                                    {{ eq.descricao|truncatechars:150 }}
                                {% elif eq.tam >= 12 %}
                                    {{ eq.descricao|truncatechars:120 }}
                                {% elif eq.tam >= 9 %}
                                    {{ eq.descricao|truncatechars:90 }}            
                                {% elif eq.tam >= 6 %}
                                    {{ eq.descricao|truncatechars:60 }}
                                {% else %}
                                    {{ eq.descricao|truncatechars:30 }}
                                {% endif %}
                            {% endif %}
                            </div>
                        </div>
                        {% if chk_stencil != "0" %}
                            {% if eq.flag_traseiro %}
                                {% if eq.imagem %}<img src="media:{{ eq.imagem }}" />{% endif %}
                            {% else %}
                                {% if eq.imagem_traseira %}<img src="media:{{ eq.imagem_traseira }}"  {% if eq.profundidade < 1.0 %}class="profundidade_curta"{% endif %} />{% endif %}
                            {% endif %}
                        {% endif %}
                   </div>
                {% endfor %}
                </div>
                {% comment %}
                <div class="div_pdu" style="height: {{ rack.altura_pxs|unlocalize }}px;">
                    {% for eq in rack.equipamentos_pdu %}
                        {% if eq.pos_col == 'TD' %}
                           {% if eq.id %}
                               <a href="{% url "admin:patrimonio_patrimonio_change" eq.id %}" onclick="return showAddAnotherPopup(this);">
                           {% endif %}
                           <div class="equip " rel="tooltip" 
                                style="margin-top:{{ eq.eixoY|unlocalize }}px; height:{{ eq.altura|unlocalize }}px;">
                                {% if eq.imagem %}<img src="media:{{ eq.imagem }}" />{% endif %}
                           </div>
                           {% if eq.id %}
                               </a>
                           {% endif %}
                        {% endif %}
                    {% endfor %}
                </div>
                {% endcomment %}
              </div>
            </div>
            {% if chk_outros == "1" and rack.equipamentos_fora_visao|length > 0 %}
                <div class="rack">
                    <div class="outros">
                        <h2><caption class="outros-title">Outros equipamentos.</caption></h2>
                        <div class="outros-content">
                            <table>
                                <tr>
                                    <th>Desc</th>
                                    <th>Tam</th>
                                    <th>Pos</th>
                                    <th>Col</th>
                                </tr>
                                {% for eq in rack.equipamentos_fora_visao %}
                                    <tr>
                                        <td>
                                            {{ eq.descricao|truncatechars:45 }}
                                        </td>
                                        <td>{{ eq.tam }}</td>
                                        <td>{{ eq.pos }}</td>
                                        <td>{{ eq.pos_col|default:'-' }}</td>
                                    </tr>
                                {% endfor %}
                            </table>
                        </div>
                    </div>
                </div>
            {% endif %}
            {# Exibindo o qadro de erros do rack #}
            {% if chk_avisos == "1" and rack.conflitos %}
                <div class="rack">
                    <div class="conflitos">
                        <span class="conflitos-title">Avisos!</span>
                        <div class="conflitos-content">
                            <table>
                                <tr>
                                    <th>EQ</th>
                                    <th>Pos</th>
                                    <th>Alt</th>
                                </tr>
                                {% for conflito in rack.conflitos %}
                                   {# Caso haja muitos erros, quebra a lista em mais colunas #}
                                   {% if not forloop.first and forloop.counter0|divisibleby:"7" %}
                                       </table>
                                       </div></div></div>
                                       <div class="rack">
                                          <div class="conflitos">
                                           <span class="conflitos-title">&nbsp;</span>
                                            <div class="conflitos-content">
                                            <table>
                                   {% endif %}
                                    <tr class="conflito-tbl-obs">
                                       <td colspan=3>{{ conflito.obs }}</td>
                                    </tr>
                                    <tr>
                                       {% if conflito.eq1 %}
                                        <td>
                                           <a href="{% url "admin:patrimonio_patrimonio_change" conflito.eq1.id %}" onclick="return showAddAnotherPopup(this);">
                                            {{ conflito.eq1.descricao }}
                                            </a>
                                        </td>
                                        <td>{{ conflito.eq1.pos }}</td>
                                        <td>{{ conflito.eq1.tam }}</td>
                                        {% endif %}
                                    </tr>
                                    <tr>
                                        {% if conflito.eq2 %}
                                        <td>
                                            <a href="{% url "admin:patrimonio_patrimonio_change" conflito.eq2.id %}" onclick="return showAddAnotherPopup(this);">
                                                {{ conflito.eq2.descricao }}
                                            </a>
                                        </td>
                                        <td>{{ conflito.eq2.pos }}</td>
                                        <td>{{ conflito.eq2.tam }}</td>
                                        {% endif %}
                                    </tr>
                                {% endfor %}
                            </table>
                        </div>
                    </div>
               </div>
           {% endif %}
        <!-- end rack -->
        {% endfor %}
    {% endfor %}
{% endfor %}
<!-- end if traseiro -->
{% endif %}
{% endblock %}

