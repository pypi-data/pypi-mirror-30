[run]
source = km3pipe
plugins = Cython.Coverage

[report]
include =
    km3pipe/*
    km3modules/*
omit =
    */tests/*
    */*/tests/*
    km3pipe/utils/*
exclude_lines =
    pragma: no cover
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    if self.debug:
    if settings.DEBUG
