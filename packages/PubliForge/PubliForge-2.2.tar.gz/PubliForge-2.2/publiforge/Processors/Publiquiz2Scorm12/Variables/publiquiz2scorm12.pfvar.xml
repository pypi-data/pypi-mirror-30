<?xml version="1.0" encoding="utf-8"?>
<publiforge version="1.0">
  <variables>
    <group>
      <label xml:lang="en">HTML 5</label>
      <label xml:lang="fr">HTML 5</label>

      <var name="onefile" type="boolean" visible="true">
        <default>false</default>
        <label xml:lang="en">Result in one file</label>
        <label xml:lang="fr">Résultat en un seul fichier</label>
      </var>

      <var name="toc_with_numbers" type="boolean" visible="true">
        <default>true</default>
        <label xml:lang="en">Numbered entries in main table of content</label>
        <label xml:lang="fr">Entrées numérotées dans le sommaire principal</label>
      </var>
      
    </group>
  </variables>
</publiforge>
