<?xml version="1.0" encoding="utf-8"?>
<publiforge version="1.0">
  <processor>

    <label xml:lang="en">Publiquiz to SCORM 1.2 conversion</label>
    <label xml:lang="fr">Conversion Publiquiz vers SCORM 1.2</label>

    <description xml:lang="en">
      Convert Publiquiz XML files into a SCORM 1.2 file.
    </description>
    <description xml:lang="fr">
      Convertit des fichiers XML Publiquiz en fichiers SCORM 1.2.
    </description>

    <module>publiforge.lib.processor.leprisme</module>

    <ancestors>
      <ancestor>Publiquiz2Html5</ancestor>
    </ancestors>

    <variables>
      <group-file>Variables/publiquiz2scorm12_module.pfvar.xml</group-file>
      <group-file>../Publiquiz2Html5/Variables/publiquiz2html5_quiz.pfvar.xml</group-file>
      <group-file>Variables/publiquiz2scorm12.pfvar.xml</group-file>
    </variables>

    <output>Result/Scorm12</output>

  </processor>
</publiforge>
