<?xml version="1.0" encoding="utf-8"?>
<publiforge version="1.0">
  <variables>
    <group>
      <label xml:lang="en">XHTML</label>
      <label xml:lang="fr">XHTML</label>

      <var name="onefile" type="boolean" visible="true">
        <default>false</default>
        <label xml:lang="en">Result in one file</label>
        <label xml:lang="fr">Résultat en un seul fichier</label>
      </var>

      <var name="nonav" type="boolean">
        <default>false</default>
        <label xml:lang="en">Without navigation bar</label>
        <label xml:lang="fr">Sans barre de navigation</label>
      </var>

      <var name="js" type="boolean" visible="true">
        <default>false</default>
        <label xml:lang="en">With JavaScript</label>
        <label xml:lang="fr">Avec JavaScript</label>
      </var>

      <!-- <var name="minify" type="boolean"> -->
