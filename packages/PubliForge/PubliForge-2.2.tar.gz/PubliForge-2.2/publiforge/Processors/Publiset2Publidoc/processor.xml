<?xml version="1.0" encoding="utf-8"?>
<publiforge version="1.0">
  <processor>

    <label xml:lang="en">Publiset XML to Publidoc/Publiquiz XML</label>
    <label xml:lang="fr">XML Publiset vers XML Publidoc/Publiquiz</label>

    <description xml:lang="en">
      Assemble a Publidoc or Publiquiz document according to a Publiset XML
      file.
    </description>
    <description xml:lang="fr">
      Assemble un document Publidoc ou Publiquiz d'apr√®s un fichier XML
      Publiset.
    </description>

    <module>publiforge.lib.processor.leprisme</module>

    <ancestors>
      <ancestor>PublidocValid</ancestor>
    </ancestors>

    <variables>
      <group-file>Variables/publiset2publidoc_module.pfvar.xml</group-file>
      <group-file>Variables/publiset2publidoc_image.pfvar.xml</group-file>
      <group-file>Variables/publiset2publidoc_multimedia.pfvar.xml</group-file>
    </variables>

    <output>Result/Xml</output>

  </processor>
</publiforge>
