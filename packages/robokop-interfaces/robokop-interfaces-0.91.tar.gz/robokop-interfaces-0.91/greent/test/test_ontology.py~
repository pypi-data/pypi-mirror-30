from greent.graph_components import KNode
from greent.services.ontology import GenericOntology

def test_generic_ontology ():
    ont = GenericOntology (ServiceContext.create_context ())
    print (ont.label ('MONDO:0009757'))
    print (ont.is_a('MONDO:0012771', 'DOID:630'))
    print (ont.xrefs ('MONDO:0012771'))
    print (ont.synonyms ('MONDO:0012771'))
    print (ont.search ('huntingtons'))
